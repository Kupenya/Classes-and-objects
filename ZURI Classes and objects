class Student:
    # [assignment] Skeleton class. Add your code here
    def __init__(self, name, age, tracks,score):
        self.name = name
        self.age = age
        self.tracks = tracks
        self.score = score
    
    def change_name(self, name):
        print(f'{self.name} is a new student')
    def change_age(self, age):
        print('He is', str(int(self.age)),'years old')
    def add_track(self, tracks):
        print(f'His tracks are {self.tracks}')
    def get_score(self, score):
        print('His score is', str(float(self.score)))
    

Bob = Student("Bob", 26, ["FE","BE"], 20.90)
Peter = Student("Peter", 34, "UI/UX, Full-stack", 37.9)
print(Bob.name)
print(Bob.age)
print(Bob.tracks)
print(Bob.score)

Peter.change_name("Peter")
Peter.change_age(34)
Peter.add_track("UI/UX, Full-stack")
Peter.get_score(37.9)
